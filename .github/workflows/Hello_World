name: Hello_World

on: 
  push:
    branches:
      - main
  workflow_dispatch:

jobs:
  build-and-test-dev:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout Code
        uses: actions/checkout@v3
      - name: Set up JDK
        uses: actions/setup-java@v3
        with:
          java-version: '11'
          distribution: temurin
      - name: Build Project
        run: mvn clean install
      - name: My Step
        run: echo "Hello World!"
